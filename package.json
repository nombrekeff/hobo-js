{
  "name": "hobo-js",
  "version": "1.0.0",
  "description": "A little utility to generate html inside your js/ts code",
  "main": "./dist/hobo.js",
  "types": "./dist/types/hobo.d.ts",
  "type": "commonjs",
  "scripts": {
    "test": "jest --coverage",
    "build": "tsc -p . -d",
    "tsdoc": "tsdoc --src=src/hobo.ts --dest=docs/API.md"
  },
  "keywords": [
    "js",
    "ts",
    "generate",
    "html",
    "from",
    "code",
    "typed",
    "template",
    "build",
    "in-code"
  ],
  "author": "nombrekeff",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.7.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsdoc-markdown": "^0.1.0",
    "typescript": "^5.2.2"
  }
}
